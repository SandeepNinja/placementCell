{
  "name": "export-to-csv",
  "version": "1.0.0",
  "description": "Easily create CSV data from json collection",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcaza/export-to-csv.git"
  },
  "scripts": {
    "build": "rm -rf output && rollup -c rollup.config.mjs",
    "e2e": "rm -rf integration/export-to-csv.js && npm run build && cp output/index.js integration/export-to-csv.js && playwright test",
    "e2e-ci": "rm -rf integration/export-to-csv.js && npm run build && cp output/index.js integration/export-to-csv.js && npx playwright install --with-deps && playwright test",
    "e2e-server": "npx http-server ./integration -p 3000",
    "test": "jest",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "export-to-csv",
    "export-to-excel",
    "csv",
    "excel",
    "libreoffice",
    "openoffice",
    "typescript",
    "zero-dependencies"
  ],
  "author": "alexcaza",
  "email": "alex@alexcaza.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexcaza/export-to-csv/issues"
  },
  "main": "./output/index.js",
  "types": "./output/index.d.ts",
  "homepage": "https://github.com/alexcaza/export-to-csv#readme",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-typescript": "^7.22.11",
    "@jest/globals": "^29.6.4",
    "@playwright/test": "^1.37.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "babel-jest": "^29.6.4",
    "http-server": "^14.1.1",
    "jest": "^29.6.4",
    "prettier": "3.0.3",
    "rollup": "^3.28.1",
    "tslib": "^2.6.2",
    "typescript": "~5.2.2"
  }
}
